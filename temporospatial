SET statement_timeout = 0;
SET lock_timeout = 0;
SET client_encoding = 'UTF8';
SET standard_conforming_strings = on;
SET check_function_bodies = false;
SET client_min_messages = warning;

--
-- Name: plpgsql; Type: EXTENSION; Schema: -; Owner: 
--

CREATE EXTENSION IF NOT EXISTS plpgsql WITH SCHEMA pg_catalog;


--
-- Name: EXTENSION plpgsql; Type: COMMENT; Schema: -; Owner: 
--

COMMENT ON EXTENSION plpgsql IS 'PL/pgSQL procedural language';


SET search_path = public, pg_catalog;

--
-- Name: date_precision; Type: TYPE; Schema: public; Owner: mouse
--

CREATE TYPE date_precision AS ENUM (
    'exact',
    'day',
    'month',
    'year',
    'decade',
    'century'
);


ALTER TYPE date_precision OWNER TO mouse;

--
-- Name: location_precision; Type: TYPE; Schema: public; Owner: mouse
--

CREATE TYPE location_precision AS ENUM (
    'exact',
    'city',
    'state',
    'country',
    'continent'
);


ALTER TYPE event_type OWNER TO mouse;

--
-- Name: event_type; Type: TYPE; Schema: public; Owner: mouse
--

CREATE TYPE location_precision AS ENUM (
    'publication',
   'birth',
   'death',
   'coronation',
   'edition',
   'law',
   'trial',
   'other'
);

ALTER TYPE location_precision OWNER TO mouse;

SET default_tablespace = '';

SET default_with_oids = false;

--
-- Name: temporospatial; Type: TABLE; Schema: public; Owner: mouse; Tablespace: 
--

CREATE TABLE temporospatial (
    id integer NOT NULL,
    created timestamp without time zone DEFAULT now(),
    updated timestamp without time zone DEFAULT now(),
    latitude double precision NOT NULL,
    longitude double precision NOT NULL,
    location_precision location_precision NOT NULL,
    location_name character varying(140) NOT NULL,
    date timestamp without time zone NOT NULL,
    date_precision date_precision NOT NULL,
    node_id integer NOT NULL,
    event_type event_type NOT NULL,
    description text NOT NULL
);


ALTER TABLE temporospatial OWNER TO mouse;

--
-- Name: temporospatial_id_seq; Type: SEQUENCE; Schema: public; Owner: mouse
--

CREATE SEQUENCE temporospatial_id_seq
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1;


ALTER TABLE temporospatial_id_seq OWNER TO mouse;

--
-- Name: temporospatial_id_seq; Type: SEQUENCE OWNED BY; Schema: public; Owner: mouse
--

ALTER SEQUENCE temporospatial_id_seq OWNED BY temporospatial.id;


--
-- Name: id; Type: DEFAULT; Schema: public; Owner: mouse
--

ALTER TABLE ONLY temporospatial ALTER COLUMN id SET DEFAULT nextval('temporospatial_id_seq'::regclass);


